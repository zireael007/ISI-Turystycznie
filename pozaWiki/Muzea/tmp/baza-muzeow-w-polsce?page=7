    window.addEvent('domready', function() {  
         
		 
  var originalFontSize = $('lewa').getStyle('font-size');
  /*
    $("resetFont").addEvent('click',function(){
    $('html').setStyle('font-size', originalFontSize);
	 $('html').setStyle('line-height', 'normal');
  });
  */
  // Increase Font Size
  $("fs_asc").addEvent('click',function(e){
    var currentFontSize = $('lewa').getStyle('font-size');
    var currentFontSizeNum = parseFloat(currentFontSize, 10);
    var newFontSize = currentFontSizeNum*1.2;
    $('lewa').setStyle('font-size', newFontSize);
	$('lewa').setStyle('line-height', 'normal');
	e.stop();
    return false;
  });
  // Decrease Font Size
  $("fs_desc").addEvent('click',function(e){
    var currentFontSize = $('lewa').getStyle('font-size');
    var currentFontSizeNum = parseFloat(currentFontSize, 10);
    var newFontSize = currentFontSizeNum*0.8;
    $('lewa').setStyle('font-size', newFontSize);
	$('lewa').setStyle('line-height', 'normal');
	e.stop();
    return false;
  });

    //menu
    new MenuMatic({'id':'menu_top'}); 

    
	
    
