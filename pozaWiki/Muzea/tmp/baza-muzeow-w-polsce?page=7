        var lng = document.getElementById('google_long').value;
            
        var latlng = new google.maps.LatLng(lat, lng);
        var myOptions = {
            zoom: 16,
            center: latlng,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        var map = new google.maps.Map(document.getElementById("mapkakontakt"), myOptions);
                        
        var marker = new google.maps.Marker({
            position: latlng, 
            map: map, 
            title:"Lokalizacja siedziby"
        });
    }
	
