    window.addEvent('domready', function() {  
         
		 
  var originalFontSize = $('lewa').getStyle('font-size');
  /*
    $("resetFont").addEvent('click',function(){
    $('html').setStyle('font-size', originalFontSize);
	 $('html').setStyle('line-height', 'normal');
  });
  */
  // Increase Font Size
  $("fs_asc").addEvent('click',function(e){
    var currentFontSize = $('lewa').getStyle('font-size');
    var currentFontSizeNum = parseFloat(currentFontSize, 10);
    var newFontSize = currentFontSizeNum*1.2;
    $('lewa').setStyle('font-size', newFontSize);
	$('lewa').setStyle('line-height', 'normal');
	e.stop();
    return false;
  });
  // Decrease Font Size
  $("fs_desc").addEvent('click',function(e){
    var currentFontSize = $('lewa').getStyle('font-size');
    var currentFontSizeNum = parseFloat(currentFontSize, 10);
    var newFontSize = currentFontSizeNum*0.8;
    $('lewa').setStyle('font-size', newFontSize);
	$('lewa').setStyle('line-height', 'normal');
	e.stop();
    return false;
  });

    //menu
    new MenuMatic({'id':'menu_top'}); 

    
	
    
     if ($('mapkakontakt')) {
        var lat = document.getElementById('google_lat').value;
        var lng = document.getElementById('google_long').value;
            
        var latlng = new google.maps.LatLng(lat, lng);
        var myOptions = {
            zoom: 16,
            center: latlng,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        var map = new google.maps.Map(document.getElementById("mapkakontakt"), myOptions);
                        
        var marker = new google.maps.Marker({
            position: latlng, 
            map: map, 
            title:"Lokalizacja siedziby"
        });
    }
	
		if ($('cookies')) {
			
			if (!Cookie.read('accepted_cookies')) {
				$('cookies').show();
				Cookie.write('accepted_cookies', 1, {
					duration: 365
				});
			}
			
			$$('#cookies .close').addEvent('click', function (e) {
				$('cookies').hide();
				e.stop();
			});
		}
	
    });
    
