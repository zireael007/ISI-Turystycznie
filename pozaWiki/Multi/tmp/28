            var api = $(".gallery").scrollable({ circular: true });
            api.click(function() {
                $(this).data("scrollable").next();
            });
            
            $(".navi a").click(function(evt) {
                evt.preventDefault();
                api.data("scrollable").seekTo(parseInt($(this).html()) - 1);

